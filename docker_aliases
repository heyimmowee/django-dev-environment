# docker
s='sudo docker'
alias d='$s'
alias d.images='$s images'
alias d.containers='$s ps -a'
alias d.runprompt='$s run -i -t $@'
alias d.run='$s run'
alias d.rmi='$s rmi'
alias d.rm='$s rm'
alias d.rmall='$s rm $($s ps -a -q)'
alias d.build='$s build --force-rm=true'
alias d.attach='$s attach'
alias d.start='$s start'
alias d.stop='$s stop'